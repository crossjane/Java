package chap02_variables;

public class _05_ShortintLong {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//1.  short타입의 변수 선언 및 초기화 
		short sNum1 = 10; 
		short sNum2 = 20; 
		
		//2. int 타입의 변수 선언 및 초기화 
		int iNum1 = 200; 
		int iNum2 = 200000000; 
		
		//3.  long 타입의 변수 선언 및 초기화 
		//L이나 l을 붙이는 것에 차이는 없다.
		//습관적으로 long 타입 변수에 값을 저장할때는 값뒤에 l,L을 붙여주는 것이 좋다.
		
		long 1Num1 = 100L; 
		long 1Num2 = 100l; 
		long 1Num3 = 100;

	}

}
